{
  "name": "react-admin-import-csv",
  "version": "1.0.0",
  "description": "CSV import button for react-admin",
  "repository": "https://github.com/benwinding/react-admin-import-csv",
  "homepage": "https://github.com/benwinding/react-admin-import-csv",
  "author": "Ben Winding",
  "license": "MIT",
  "private": false,
  "files": [
    "*.md",
    "lib",
    "esm",
    "src"
  ],
  "main": "lib/index",
  "module": "esm/index.js",
  "scripts": {
    "build": "yarn run build-cjs && yarn run build-esm",
    "build-watch": "yarn build-for-demo -w",
    "build-for-demo": "rimraf ./demo/src/build-watch && tsc --outDir demo/src/build-watch",
    "build-cjs": "rimraf ./lib && tsc",
    "build-esm": "rimraf ./esm && tsc --outDir esm --module es2015",
    "test": "jest --collectCoverage",
    "test:watch": "jest --watch",
    "test:silent": "npx jest 2>&1 | grep 'â—'",
    "test:badges": "jest-coverage-badges",
    "clean": "rimraf dist"
  },
  "peerDependencies": {
    "react-admin": "4.x"
  },
  "dependencies": {
    "papaparse": "5.x",
    "ramda": "^0.28.0",
    "react-redux": "^8.0.1"
  },
  "devDependencies": {
    "@types/jest": "27.4.1",
    "@types/node": "17.0.25",
    "@types/papaparse": "5.x",
    "@types/ramda": "0.28.11",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "jest-coverage-badges": "^1.1.2",
    "ra-data-fakerest": "^4.0.1",
    "ra-language-chinese": "^2.0.10",
    "ra-language-russian": "^3.13.5",
    "ra-language-spanish": "1.x",
    "react": "^17.0.0",
    "react-admin": "4.0.x",
    "react-dom": "^17.0.0",
    "react-scripts": "^5.0.1",
    "source-map-loader": "^3.0.1",
    "ts-jest": "^27.x.x",
    "typescript": "4.x"
  }
}
